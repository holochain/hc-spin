{
  "name": "@holochain/hc-spin-rust-utils",
  "version": "0.200.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "hc-spin-rust-utils",
    "triples": {}
  },
  "license": "MIT",
  "description": "Rust node add-ons used by @holochain/hc-spin",
  "devDependencies": {
    "@napi-rs/cli": "^2.16.5",
    "ava": "^5.1.1"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:local": "napi build --platform --release && rimraf dist && mkdir dist && cp package.json dist/ && cp index.js dist/ && cp index.d.ts dist/ && cp hc-spin-rust-utils* dist/",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version"
  }
